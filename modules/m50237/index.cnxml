                    </m:mrow>
                    <m:msub>
                      <m:mi fontstyle="italic">Δv</m:mi>
                      <m:mrow>
                        <m:mn>0</m:mn>
                      </m:mrow>
                    </m:msub>
                    <m:msub>
                      <m:mo stretchy="false">)</m:mo>
                      <m:mrow>
                        <m:mrow>
                          <m:mn>1</m:mn>
                          <m:mo stretchy="false">/</m:mo>
